PROGRAM subprog;

FUNCTION b1:LONGINT;
BEGIN
	WRITELN(1);
	EXIT(1)
END;

FUNCTION b2:LONGINT;
BEGIN
	WRITELN(2);
	EXIT(b1())
END;

FUNCTION b3:LONGINT;
BEGIN
	WRITELN(3);
	EXIT(b1()+b2())
END;

FUNCTION b4 :LONGINT;
BEGIN
	WRITELN(4);
	EXIT(b1()+b2()+b3())
END;


BEGIN
	WRITELN(b1()+b2()+b3()+b4())
END.
